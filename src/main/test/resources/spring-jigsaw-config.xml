<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:jigsaw="http://www.guanjian.com/schema/jigsaw"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                                   http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
                                   http://www.guanjian.com/schema/jigsaw
                                   http://www.guanjian.com/schema/jigsaw.xsd">

    <jigsaw:layout>
        <jigsaw:panel id="" height="100" width="100"/>
        <jigsaw:layer />
    </jigsaw:layout>

    <jigsaw:layer></jigsaw:layer>
    <jigsaw:panel></jigsaw:panel>

    <jigsaw:text id="textDemo" rgbColor="11,11,11" fontSize="14" fontText="测试" fontFormat="宋体"/>
    <jigsaw:element id="elementDemo" width="100" height="100" coordinateX="0" coordinateY="0"/>
</beans>
